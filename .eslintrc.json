{
    "env": {
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": "off",
        "eqeqeq": "error",
        "guard-for-in": "error",
        "curly": "error",
        "indent": ["error", 4],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "double"],
        "semi": ["error", "always"]
    }
}
